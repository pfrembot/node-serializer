{
  "name": "node-serializer",
  "version": "1.0.0",
  "description": "Json serialization library for node inspired by Symfony's Serializer Component (https://symfony.com/doc/current/components/serializer.html)",
  "main": "index.js",
  "scripts": {
    "tests": "mocha --require babel-core/register --recursive ./test",
    "build": "babel src --out-dir lib",
    "flow": "flow"
  },
  "keywords": [
    "json",
    "gson",
    "serialization",
    "deserialization"
  ],
  "author": "Edward Pfremmer <epfremmer@gmail.com>",
  "homepage": "https://github.com/pfrembot/node-serializer",
  "repository": "git://github.com/pfrembot/node-serializer.git",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.62.0",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.10",
    "xml2js": "^0.4.19"
  }
}
